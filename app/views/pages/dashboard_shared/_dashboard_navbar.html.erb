<div class="dashboard-navbar">
  <div class="dashboard-navbar-buttons">
    <%= link_to home_path do %>
      <i class="fa-solid fa-house"></i>
    <% end %>
    <%= link_to dashboard_path,
                class: "icon-dashboard-nav"  do %>
      <i class="fa-regular fa-plus" id="plus-icon selected-nav"></i>
    <% end %>
    <%= link_to agency_listings_path(@agency),
                class: "icon-dashboard-nav" do %>
      <i class="fa-solid fa-envelope"></i>
    <% end %>

  </div>

  <div class="nav-avatar">
    <% if user_signed_in? %>
        <%= link_to "Log out", destroy_user_session_path, data: {turbo_method: :delete}, class: "navbar-login" %>
