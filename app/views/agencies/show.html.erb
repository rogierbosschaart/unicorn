
<div class="container my-4">
    <h1><%= @agency.name %></h1>
    <%= @clients.each do |client| %>
        <p><strong>Client Name:</strong> <%= client.name %></p>
    <% end %>

  
</div>
<div class="col-md-6">
    <div class="card mb-4 shadow-sm">
        <div class="card-header bg-info text-white">
          <h2 class="h5 mb-0">Users</h2>
        </div>
        <div class="card-body">
          <% if @agency.users.any? %>
            <ul class="list-group list-group-flush">
              <% @agency.users.each do |user| %>
                <li class="list-group-item mb-3 p-3 border rounded bg-light">
                  <p class="mb-1"><strong>First Name:</strong> <%= user.first_name %></p>
                  <p class="mb-0"><strong>Last Name:</strong> <%= user.last_name %></p>
                </li>
              <% end %>
            </ul>
          <% else %>
            <p class="text-muted">This agency dont have a model.</p>
          <% end %>
        </div>
    </div>
</div>